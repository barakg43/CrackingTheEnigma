<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<ScrollPane fitToHeight="true" fitToWidth="true" maxHeight="520.0" maxWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.CurrentCode.CurrentCodeController">
   <content>
      <AnchorPane>
         <children>
            <Label fx:id="SelectedReflectorLabel" layoutX="19.0" layoutY="342.0" styleClass="currentReflector-button" text="Selected Reflector:" />
            <Label fx:id="SelectedReflector" layoutX="193.0" layoutY="342.0" styleClass="currentReflector-button" text="Label" />
            <Label fx:id="RotorsAndPositionsLabel" layoutX="14.0" layoutY="127.0" styleClass="currentRotors-button" text="Rotors and Positions:" />
            <TableView fx:id="RotorsAndPositionsTable" layoutX="191.0" layoutY="127.0" prefHeight="200.0" prefWidth="148.0">
              <columns>
                <TableColumn fx:id="RotorIDColumn" prefWidth="75.0" text="Rotor ID" />
                <TableColumn fx:id="PositionColumn" prefWidth="69.0" text="Position" />
              </columns>
            </TableView>
            <ListView fx:id="PairsListView" layoutX="187.0" layoutY="398.0" prefHeight="99.0" prefWidth="92.0" stylesheets="@../CurrentCode/CurrentCodeCSS.css" visible="false" />
            <Label fx:id="PlugBoardPairsLabel" layoutX="20.0" layoutY="398.0" styleClass="currentPlugBoardPairs-button" text="PlugBoard Pairs:" />
            <Label fx:id="NoPlubBoardPairsLabel" layoutX="192.0" layoutY="398.0" styleClass="currentPlugBoardPairs-button" text="No plugBoard pairs" visible="false" />
            <Pane fx:id="CurrentCodePane" layoutX="1.0" layoutY="14.0" prefHeight="99.0" prefWidth="385.0">
               <children>
                  <Label fx:id="CurrentCodeLabel" layoutX="14.0" layoutY="14.0" styleClass="currentCode-Label" text="Current Code:" />
                  <Pane layoutX="14.0" layoutY="67.0">
                     <children>
                        <fx:include fx:id="currCode" source="../Code/codeTemplate.fxml" />
                     </children>
                  </Pane>
               </children>
            </Pane>
         </children>
      </AnchorPane>
   </content>
</ScrollPane>
