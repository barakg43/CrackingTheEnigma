<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<VBox spacing="10.0" stylesheets="@ApplicationTitle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ApplicationController">
   <children>
      <HBox spacing="20.0">
         <children>
            <VBox>
               <children>
                  <Label fx:id="UboatTitle" maxWidth="1.7976931348623157E308" prefHeight="25.0" styleClass="title-label" text="UBoat Application">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Label>
                  <Label fx:id="helloUserLabel" styleClass="title-label" text="Hello">
                     <VBox.margin>
                        <Insets left="15.0" />
                     </VBox.margin>
                  </Label>
               </children>
            </VBox>
            <AnchorPane prefHeight="60.0" prefWidth="330.0">
               <children>
                  <Button fx:id="clearButton" layoutX="157.0" layoutY="15.0" mnemonicParsing="false" onAction="#clearDataAction" prefHeight="31.0" prefWidth="119.0" text="Clear Data" visible="false" />
                  <Button fx:id="chatButton" layoutX="14.0" layoutY="15.0" mnemonicParsing="false" onAction="#openChatWindow" text="Chat Window" />
               </children>
            </AnchorPane>
         </children>
      </HBox>
      <FlowPane fx:id="mainPanel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
   </children>
</VBox>
