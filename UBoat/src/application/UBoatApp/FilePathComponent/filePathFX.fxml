<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<ScrollPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.UBoatApp.FilePathComponent.FilePathController">
   <content>
      <AnchorPane>
         <children>
              <Button fx:id="LoadFileButton" layoutX="744.0" layoutY="30.0" mnemonicParsing="false" onAction="#LoadFileButtonActionListener" prefHeight="31.0" prefWidth="211.0" styleClass="file-lable" stylesheets="@filePathCSS.css" text="Open File" />
              <Label fx:id="FileLabel" layoutX="11.0" layoutY="23.0" prefHeight="42.0" prefWidth="78.0" styleClass="file-lable" stylesheets="@filePathCSS.css" text="File:" />
              <Label fx:id="SelectedFilePath" layoutX="50.0" layoutY="30.0" prefHeight="31.0" prefWidth="666.0" styleClass="file-lable" stylesheets="@filePathCSS.css" />
            <Label id="First-Load-File-Label" fx:id="FirstLoadFileLabel" layoutX="11.0" layoutY="77.0" stylesheets="@filePathCSS.css" text="You need first load the machine from file" />
         </children>
      </AnchorPane>
   </content>
</ScrollPane>
