<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.UBoatApp.ContestTab.ContestController">
   <children>
      <StackPane>
         <children>
            <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="527.0" prefWidth="1311.0">
               <children>
                  <AnchorPane prefHeight="199.0" prefWidth="103.0">
                     <children>
                        <Button id="ready-button" fx:id="readyButton" layoutX="29.0" layoutY="430.0" mnemonicParsing="false" onAction="#startBattlefieldContest" prefHeight="36.0" prefWidth="103.0" stylesheets="@contestCSS.css" text="ready" />
                        <fx:include fx:id="EncryptComponent" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" source="encryptComponent/EncryptTab.fxml" />
                     </children>
                  </AnchorPane>
                  <fx:include fx:id="teamsStatusComponent" source="TeamsStatus/TeamStatus.fxml" />
               </children>
            </HBox>
         </children>
      </StackPane>
      <fx:include fx:id="candidatesStatusComponent" source="CandidateStatus/CandidateStatus.fxml" />
      <ButtonBar prefHeight="40.0" prefWidth="200.0">
        <buttons>
            <Button id="logout-button" fx:id="logoutButton" mnemonicParsing="false" onAction="#logoutButtonOnAction" prefHeight="36.0" prefWidth="105.0" stylesheets="@contestCSS.css" text="Logout" />
        </buttons>
         <padding>
            <Insets bottom="10.0" right="50.0" top="10.0" />
         </padding>
      </ButtonBar>
   </children>
</VBox>
