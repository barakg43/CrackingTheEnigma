<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainAgentApp.MainAgentController">
   <children>
      <HBox spacing="20.0">
         <children>
            <VBox stylesheets="@MainAgentCSS.css">
               <children>
                  <Label fx:id="agentTitle" styleClass="title-label" text="Agent Application">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Label>
                  <Label fx:id="helloUserNameLabel" styleClass="title-label" text="Hello">
                     <VBox.margin>
                        <Insets left="15.0" />
                     </VBox.margin>
                  </Label>
               </children>
            </VBox>
            <AnchorPane prefHeight="60.0" prefWidth="330.0">
               <children>
                  <Button fx:id="clearButton" layoutX="157.0" layoutY="15.0" mnemonicParsing="false" onAction="#clearDataAction" prefHeight="31.0" prefWidth="119.0" text="Clear Data" visible="false" />
                  <Button fx:id="chatButton" layoutX="14.0" layoutY="15.0" mnemonicParsing="false" onAction="#openChatWindow" text="Chat Window" />
               </children>
            </AnchorPane>
         </children>
      </HBox>
      <FlowPane fx:id="mainPanel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="15.0" />
         </VBox.margin></FlowPane>
   </children>
</VBox>
